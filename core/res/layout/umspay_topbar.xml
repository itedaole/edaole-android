<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/re_main_log"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/umspay_topbar_bg" >

    <Button
        android:id="@+id/btn_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="10dp"
        android:background="@drawable/umspay_btn_home"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/ig_main_log1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/umspay_top_logo" />

    <TextView
        android:id="@+id/top_title"
        android:layout_width="wrap_content"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true" />

    <Button
        android:id="@+id/btn_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        android:background="@drawable/umspay_btn_back"
        android:visibility="gone" />

</RelativeLayout>